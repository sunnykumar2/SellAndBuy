<% include ./partials/header %>

<div class="container" align='Center'>
    <h1 align='Center'>Login!</h1>
    <form action="/login" method="POST">
        <div>
            <input type="text" name="username" placeholder="username" style="border-color: blue;">
        </div>
        <div>
            <input type="password" name="password" placeholder="password" style="border-color: blue;">
        </div>
        <div>
            <input type="button" value="Login" class="btn btn-primary">
        </div>
    </form>
</div>